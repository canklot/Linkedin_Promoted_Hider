{
    "manifest_version": 3,
    "name": "Linkedin Promoted Job Hider Extension",
    "version": "1.0",
    "description": "Hides promoted jobs on linkedin",
    "permissions": [],
    "content_scripts": [
        {
            "matches": [
                "https://www.linkedin.com/*"
            ],
            "js": [
                "main.js"
            ],
            "runat": "document_end"
        }
    ],
    "host_permissions": [
        "https://www.linkedin.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "author": "Canklot",
    "homepage_url": "https://github.com/canklot"
}